    <script src="https://www.paypal.com/sdk/js?client-id=AShHA7LRHXBp7nwEXq8M4l4LOGXPl2Ht9Ynrm5S_tOvtjTZHBXYrb6qaSNJqj-DuUpuQ1uAwT4syT_TQ&currency=USD">
    
    <script>
        // Network Tools Functions
        async function lookupIP() {
            const ip = document.getElementById('ipInput').value.trim();
            const resultDiv = document.getElementById('ipResult');
            
            if (!ip) {
                resultDiv.style.display = 'block';
                resultDiv.innerHTML = '<span style="color: #ff0000;">Please enter an IP address</span>';
                return;
            }
            
            resultDiv.style.display = 'block';
            resultDiv.innerHTML = '<span style="color: #ffff00;">‚ö° Scanning...</span>';
            
            try {
                const response = await fetch(`https://ipapi.co/${ip}/json/`);
                const data = await response.json();
                
                if (data.error) {
                    resultDiv.innerHTML = `<span style="color: #ff0000;">Error: ${data.reason}</span>`;
                } else {
                    resultDiv.innerHTML = `
                        <div style="font-family: 'Courier New', Courier, monospace; line-height: 1.8;">
                            <div><span style="color: #00ff00;">IP:</span> ${data.ip}</div>
                            <div><span style="color: #00ff00;">City:</span> ${data.city || 'N/A'}</div>
                            <div><span style="color: #00ff00;">Region:</span> ${data.region || 'N/A'}</div>
                            <div><span style="color: #00ff00;">Country:</span> ${data.country_name || 'N/A'}</div>
                            <div><span style="color: #00ff00;">ISP:</span> ${data.org || 'N/A'}</div>
                            <div><span style="color: #00ff00;">Timezone:</span> ${data.timezone || 'N/A'}</div>
                        </div>
                    `;
                }
            } catch (error) {
                resultDiv.innerHTML = '<span style="color: #ff0000;">Error: Failed to lookup IP</span>';
            }
        }
        
        async function lookupMAC() {
            const mac = document.getElementById('macInput').value.trim();
            const resultDiv = document.getElementById('macResult');
            
            if (!mac) {
                resultDiv.style.display = 'block';
                resultDiv.innerHTML = '<span style="color: #ff0000;">Please enter a MAC address</span>';
                return;
            }
            
            resultDiv.style.display = 'block';
            resultDiv.innerHTML = '<span style="color: #ffff00;">‚ö° Scanning...</span>';
            
            try {
                const cleanMac = mac.replace(/[:-]/g, '').substring(0, 6);
                const response = await fetch(`https://api.macvendors.com/${mac}`);
                const vendor = await response.text();
                
                if (response.ok) {
                    resultDiv.innerHTML = `
                        <div style="font-family: 'Courier New', Courier, monospace; line-height: 1.8;">
                            <div><span style="color: #00ff00;">MAC:</span> ${mac}</div>
                            <div><span style="color: #00ff00;">Vendor:</span> ${vendor}</div>
                            <div><span style="color: #00ff00;">OUI:</span> ${cleanMac.toUpperCase()}</div>
                        </div>
                    `;
                } else {
                    resultDiv.innerHTML = '<span style="color: #ff0000;">Vendor not found or invalid MAC address</span>';
                }
            } catch (error) {
                resultDiv.innerHTML = '<span style="color: #ff0000;">Error: Failed to lookup MAC address</span>';
            }
        }
        
        async function getMyIP() {
            const resultDiv = document.getElementById('myipResult');
            resultDiv.style.display = 'block';
            resultDiv.innerHTML = '<span style="color: #ffff00;">‚ö° Fetching your IP info...</span>';
            
            try {
                const response = await fetch('https://ipapi.co/json/');
                const data = await response.json();
                
                resultDiv.innerHTML = `
                    <div style="font-family: 'Courier New', Courier, monospace; line-height: 1.8;">
                        <div><span style="color: #00ff00;">Your IP:</span> ${data.ip}</div>
                        <div><span style="color: #00ff00;">City:</span> ${data.city || 'N/A'}</div>
                        <div><span style="color: #00ff00;">Region:</span> ${data.region || 'N/A'}</div>
                        <div><span style="color: #00ff00;">Country:</span> ${data.country_name || 'N/A'}</div>
                        <div><span style="color: #00ff00;">ISP:</span> ${data.org || 'N/A'}</div>
                        <div><span style="color: #00ff00;">Timezone:</span> ${data.timezone || 'N/A'}</div>
                        <div><span style="color: #00ff00;">Postal:</span> ${data.postal || 'N/A'}</div>
                    </div>
                `;
            } catch (error) {
                resultDiv.innerHTML = '<span style="color: #ff0000;">Error: Failed to fetch IP info</span>';
            }
        }
        
        async function lookupDomain() {
            const domain = document.getElementById('domainInput').value.trim();
            const resultDiv = document.getElementById('domainResult');
            
            if (!domain) {
                resultDiv.style.display = 'block';
                resultDiv.innerHTML = '<span style="color: #ff0000;">Please enter a domain name</span>';
                return;
            }
            
            resultDiv.style.display = 'block';
            resultDiv.innerHTML = '<span style="color: #ffff00;">‚ö° Resolving...</span>';
            
            try {
                const response = await fetch(`https://dns.google/resolve?name=${domain}&type=A`);
                const data = await response.json();
                
                if (data.Answer && data.Answer.length > 0) {
                    const ips = data.Answer.filter(a => a.type === 1).map(a => a.data).join(', ');
                    resultDiv.innerHTML = `
                        <div style="font-family: 'Courier New', Courier, monospace; line-height: 1.8;">
                            <div><span style="color: #00ff00;">Domain:</span> ${domain}</div>
                            <div><span style="color: #00ff00;">IP Address(es):</span> ${ips || 'N/A'}</div>
                            <div><span style="color: #00ff00;">Records Found:</span> ${data.Answer.length}</div>
                        </div>
                    `;
                } else {
                    resultDiv.innerHTML = '<span style="color: #ff0000;">No DNS records found</span>';
                }
            } catch (error) {
                resultDiv.innerHTML = '<span style="color: #ff0000;">Error: Failed to resolve domain</span>';
            }
        }
        
        // PayPal Integration Functions
        let selectedAmount = 0;
        
        function selectAmount(amount) {
            selectedAmount = amount;
            
            // Remove selection from all cards
            document.querySelectorAll('.need-card').forEach(card => {
                card.classList.remove('selected');
            });
            
            // Add selection to clicked card
            event.currentTarget.classList.add('selected');
            
            // Clear custom amount
            document.getElementById('customAmount').value = '';
            
            // Re-render PayPal button with new amount
            renderPayPalButton();
        }
        
        // Listen for custom amount changes
        document.getElementById('customAmount').addEventListener('input', function(e) {
            selectedAmount = parseFloat(e.target.value) || 0;
            
            // Remove selection from preset cards
            document.querySelectorAll('.need-card').forEach(card => {
                card.classList.remove('selected');
            });
            
            if (selectedAmount > 0) {
                renderPayPalButton();
            }
        });
        
        function renderPayPalButton() {
            // Clear existing button
            document.getElementById('paypal-button-container').innerHTML = '';
            
            if (selectedAmount <= 0) {
                return;
            }
            
            // Render PayPal button
            paypal.Buttons({
                createOrder: function(data, actions) {
                    return actions.order.create({
                        purchase_units: [{
                            amount: {
                                value: selectedAmount.toFixed(2)
                            },
                            description: 'Support for essential needs'
                        }]
                    });
                },
                onApprove: function(data, actions) {
                    return actions.order.capture().then(function(details) {
                        // Show success message
                        document.getElementById('success-message').style.display = 'block';
                        
                        // Scroll to success message
                        document.getElementById('success-message').scrollIntoView({ 
                            behavior: 'smooth',
                            block: 'center'
                        });
                        
                        // Reset form after a delay
                        setTimeout(function() {
                            selectedAmount = 0;
                            document.getElementById('customAmount').value = '';
                            document.querySelectorAll('.need-card').forEach(card => {
                                card.classList.remove('selected');
                            });
                            document.getElementById('paypal-button-container').innerHTML = '';
                        }, 3000);
                    });
                },
                onError: function(err) {
                    alert('An error occurred with the payment. Please try again.');
                    console.error(err);
                }
            }).render('#paypal-button-container');
        }
        
        // Initial render message
        document.getElementById('paypal-button-container').innerHTML = '<p style="text-align: center; color: #666; padding: 2rem;">Select an amount above to see payment options</p>';
        
        // Add event listeners for tool buttons
        document.getElementById('ipLookupBtn').addEventListener('click', lookupIP);
        document.getElementById('macLookupBtn').addEventListener('click', lookupMAC);
        document.getElementById('myIpBtn').addEventListener('click', getMyIP);
        document.getElementById('domainLookupBtn').addEventListener('click', lookupDomain);
        
        // Load PayPal SDK after tools are defined
        const script = document.createElement('script');
        script.src = 'https://www.paypal.com/sdk/js?client-id=AShHA7LRHXBp7nwEXq8M4l4LOGXPl2Ht9Ynrm5S_tOvtjTZHBXYrb6qaSNJqj-DuUpuQ1uAwT4syT_TQ&currency=USD';
        document.head.appendChild(script);
    </script>
</body>
</html>
